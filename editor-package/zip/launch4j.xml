<launch4jConfig>
    <headerType>gui</headerType>
    <dontWrapJar>true</dontWrapJar>
    <icon>icon.ico</icon>
    <classPath>
        <mainClass>kkckkc.jsourcepad.Bootstrap</mainClass>
        <cp>lib/*.jar</cp>
    </classPath>
    <chdir>.</chdir>
    <jre>
        <minVersion>1.6.0</minVersion>
        <opt>-Dlaunch4j.exedir="%EXEDIR%"</opt>
        <opt>-Dlaunch4j.exefile="%EXEFILE%"</opt>
        <opt>-Djava.util.logging.config.class=kkckkc.jsourcepad.BootstrapLogger</opt>
        <opt>-DexecutableName=jsourcepad.exe</opt>
    </jre>
</launch4jConfig>